#CODE:ROLLER COASTER CODE

print("Welcome to the rollercoaster")
height = int(input("Enter your height in cm:"))
bill = 0
if height >= 120:
  print("You can ride the rollercoaster")
  age = int(input("Enter your age:"))
  if age <= 12:
    bill = 5
    print("Child tickets 5$")
  elif age <= 18:
    bill = 7
    print("Youth tickets 7$")
  elif age >= 45 and age <= 55:
    bill = 0
    print("free ride for you")
  else:
    bill = 12
    print("Adult tickets 12$")
  photo = input("Do you want to take photos? Y or N.")
  if photo == "Y" :
    bill += 3
    print("Your final bill is $ %d" %bill)
  else:
    print("Your final bill is $ %d" %bill)
else:
  print("You can't ride rollercoaste")
